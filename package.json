{
  "scripts": {
    "dev": "npx webpack --mode development & bundle exec jekyll serve",
    "build:development": "yarn && npx webpack --mode development && bundle exec jekyll build --config _config.yml,_config_development.yml",
    "build:production": "yarn && npx webpack --mode production && JEKYLL_ENV=production bundle exec jekyll build"
  },
